autoload zmv

alias pbcopy='xclip -selection clipboard'
alias pbpaste='xclip -selection clipboard -o'
alias ack='ack-grep'

export VISUAL=vim
export EDITOR="$VISUAL"

prompt paradox

function reload {
    (
      echo "Updating dotfiles..."
      cd ~/.dotfiles
      git pull
    )
    source ~/.zshrc
    tmux source-file ~/.tmux.conf
}
